<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e7f6f9"
    tools:context=".result">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout59"
        android:layout_width="323dp"
        android:layout_height="76dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/rounnded_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout60"
        android:layout_width="325dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_white_background"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout59"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout59"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout59">

        <TextView
            android:id="@+id/p5textView"
            android:layout_width="326dp"
            android:layout_height="157dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/rounded_white_background"
            android:padding="20dp"
            android:text="Candidate Application Form - VYZEN"
            android:textSize="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/p5view3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#808080"
            app:layout_constraintEnd_toEndOf="@+id/p5textView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/p5textView" />

        <TextView
            android:id="@+id/p5textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="20dp"
            android:gravity="center"
            android:text="You Have Succesdully SUbmitted"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/p5view3" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>